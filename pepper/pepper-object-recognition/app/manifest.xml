    <description lang="cs_CZ">A simple app consisting of a collaborative dialogue talking to a service which runs image recognition using remote GPU-powered machine.</description>
    </descriptions>
  <supportedLanguages>
    <language>en_US</language>
    <language>cs_CZ</language>
    </supportedLanguages>
  <descriptionLanguages>
    <language>en_US</language>
    <language>cs_CZ</language>
    </descriptionLanguages>
  <contents>
    <behaviorContent path="testrun">
      <nature>interactive</nature>
      <descriptions>
        <description lang="en_US">Launch this to test your collaborative dialog in isolation (easier for debugging)</description>
        </descriptions>
      <permissions />
